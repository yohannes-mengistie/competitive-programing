class Solution(object):
    def sortColors(self, nums):
        """
        :type nums: List[int]
        :rtype: None Do not return anything, modify nums in-place instead.
        """
        for j in range(len(nums)):
            color = nums[j]
            i = j
            while i>0 and nums[i-1]>color:
                nums[i]=nums[i-1]
                i -=1

            nums[i] = color
        print(nums)
